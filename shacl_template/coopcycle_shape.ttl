@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <https://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .

schema:ProfessionalServiceShape
    a sh:NodeShape ;
    sh:targetClass schema:ProfessionalService ;
    sh:property [
        sh:path schema:latitude ;
        sh:datatype xsd:double ;
        sh:minCount 1 ;
    ] ;
    sh:property [
        sh:path schema:longitude ;
        sh:datatype xsd:double ;
        sh:minCount 1 ;
    ] ;
    sh:property [
        sh:path schema:legalName ;
        sh:datatype xsd:string ;
        sh:minCount 1 ;
    ] ;
    sh:property [
        sh:path schema:areaServed ;
        sh:datatype xsd:string ;
        sh:minCount 1 ;
    ] ;
    sh:property [
        sh:path schema:contactPoint ;
        sh:datatype xsd:string ;
        sh:minCount 1 ;
    ] ;
    sh:property [
        sh:path schema:email ;
        sh:datatype xsd:string ;
    ] ;
    sh:property [
        sh:path schema:knowsLanguage ;
        sh:datatype xsd:string ;
        sh:minCount 1 ;
    ] ;
    sh:property [
        sh:path schema:location ;
        sh:datatype xsd:string ;
        sh:minCount 1 ;
    ] ;
    sh:property [
        sh:path schema:memberOf ;
        sh:node schema:OrganizationShape ;
        sh:minCount 1 ;
    ] ;
    sh:property [
        sh:path schema:url ;
        sh:datatype xsd:anyURI ;
    ] .

schema:OrganizationShape
    a sh:NodeShape ;
    sh:targetClass schema:Organization ;
    sh:property [
        sh:path schema:name ;
        sh:datatype xsd:string ;
        sh:minCount 1 ;
    ] ;
    sh:property [
        sh:path schema:url ;
        sh:datatype xsd:anyURI ;
        sh:minCount 1 ;
    ] .